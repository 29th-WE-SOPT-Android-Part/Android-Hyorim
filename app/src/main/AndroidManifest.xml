<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyorim.sopt_assigmnet_1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SOPT_Assigmnet_1"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.setting.SettingActivity"></activity>
        <activity android:name=".ui.onboading.OnBoardingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.signup.SignUpActivity" />
        <activity android:name=".ui.home.HomeActivity" />
        <activity android:name=".ui.signin.SignInActivity" />
    </application>

</manifest>